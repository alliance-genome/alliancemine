    <source name="go" type="go">
      <property name="src.data.file" location="/root/data/fms/ONTOLOGY_GO.obo"/>
    </source>

    <source name="eco" type="eco">
       <property name="src.data.file" location="/root/data/fms/ONTOLOGY_ECO.obo"/>
